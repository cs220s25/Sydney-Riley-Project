[Unit]
Description=Discord Bot App
After=network.target
Wants=network.target

[Service]
User=ec2-user
Group=ec2-user
WorkingDirectory=/home/ec2-user/Sydney-Riley-Project
ExecStartPre=/usr/bin/mvn exec:java -Dexec.mainClass="edu.moravian.LoadData"
ExecStart=/usr/bin/java -jar /home/ec2-user/Sydney-Riley-Project/target/dbot-1.0-SNAPSHOT-jar-with-dependencies.jar
Restart=on-failure
RestartSec=5

[Install]
WantedBy=multi-user.target
